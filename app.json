{
  "expo": {
    "entryPoint": "./src/App.tsx",
    "name": "Anonymous File Upload",
    "slug": "Anonymous_File_Upload",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#3c4347"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.jmqm.anonymousfileupload",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.jmqm.anonymousfileupload",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
